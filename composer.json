{
    "name": "laravel-bridge/yii",
    "license": "MIT",
    "description": "The bridge for Laravel on Yii framework",
    "keywords": [
        "template"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "laravel-bridge/support": "~0.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4 | ^5 | ^6",
        "psy/psysh": "^0.9.9",
        "squizlabs/php_codesniffer": "^3.5",
        "yiisoft/yii": "1.1.14"
    },
    "autoload": {
        "psr-4": {
            "LaravelBridge\\Yii\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        },
        "classmap": [
            "tests/Fixture"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
