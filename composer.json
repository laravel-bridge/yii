{
    "name": "laravel-bridge/yii",
    "license": "MIT",
    "description": "The bridge for Laravel on Yii framework",
    "keywords": [
        "template"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "yiisoft/yii": "1.1.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^4 | ^5 | ^6",
        "squizlabs/php_codesniffer": "^3.4",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "LaravelBridge\\Yii\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
